{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "handle": "compA",
            "name": "CompanyA",
            "num_employees": 1,
            "description": "The Company",
            "logo_url": "http://example.com/image.jpg",
            "_token": "tokenstring"
        }
    ],
    "required": [
        "handle",
        "name",
        "num_employees",
        "description",
        "logo_url",
        "_token"
    ],
    "properties": {
        "handle": {
            "$id": "#/properties/handle",
            "type": "string",
            "title": "The handle schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "compA"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "CompanyA"
            ]
        },
        "num_employees": {
            "$id": "#/properties/num_employees",
            "type": "integer",
            "title": "The num_employees schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "The Company"
            ]
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "http://example.com/image.jpg"
            ]
        },
        "_token": {
            "$id": "#/properties/_token",
            "type": "string",
            "title": "The _token schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "tokenstring"
            ]
        }
    },
    "additionalProperties": true
}